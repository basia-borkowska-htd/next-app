{
  "bracketSpacing": true,
  "printWidth": 120,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "pages/(.*)$",
    "components/(.*)$",
    "hooks/(.*)$",
    "types/(.*)$",
    "enums/(.*)$",
    "assets/(.*)$",
    "utils/(.*)$",
    "^[./*.*]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
